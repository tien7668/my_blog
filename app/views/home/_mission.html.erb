    <div class="row">
      <div class="col-lg-3 col-sm-6">
        <div class="single_service_part">
          <div class="single_service_part_iner">
            <span class="ti-mobile"></span>
            <h3><%= t :mission_1_title %></h3>
            <p><%= t :mission_1_content %></p>
          </div>
        </div>
      </div>
      <div class="col-lg-3 col-sm-6">

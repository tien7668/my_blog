<!-- cta part start-->
<section class="cta_part">
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-12">
        <div class="cta_part_iner">
          <div class="cta_part_text">
            <p><%= t :mission_title %></p>
            <h1><%= t :mission %></h1>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- cta part end-->
